function NoaMoreTp()
while getgenv().NoMoreTp == true do
    
    for _,v in pairs(game:GetService("Workspace").OldBuildingTp2:GetDescendants()) do
if v:IsA("TouchTransmitter") then
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0) --0 is touch
wait()
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1) -- 1 is untouch


    
    
teleportTO(game:GetService("Workspace").OldBuildingTp2.CFrame)
wait()
teleportTO(game:GetService("Workspace").Part.CFrame)
end
end
end
end



function NoaMoreTpv2()
while getgenv().NoMoreTpv2 == true do
    
    for _,v in pairs(game:GetService("Workspace").OldBuildingTp2:GetDescendants()) do
if v:IsA("TouchTransmitter") then
firetouchinterest(game.Players.LocalPlayer.Character.Torso, v.Parent, 0) --0 is touch
wait()
firetouchinterest(game.Players.LocalPlayer.Character.Torso, v.Parent, 1) -- 1 is untouch

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-141.5457, 430.599976, 5485.75049, -0.999583125, -7.39692974e-09, 0.0288711432, -7.10421233e-09, 1, 1.02413376e-08, -0.0288711432, 1.0031961e-08, -0.999583125)

firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0) --0 is touch
wait()
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1) -- 1 is untouch

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-141.5457, 430.599976, 5485.75049, -0.999583125, -7.39692974e-09, 0.0288711432, -7.10421233e-09, 1, 1.02413376e-08, -0.0288711432, 1.0031961e-08, -0.999583125)

firetouchinterest(game.Players.LocalPlayer.Character.Head, v.Parent, 0) --0 is touch
wait()
firetouchinterest(game.Players.LocalPlayer.Character.Head, v.Parent, 1) -- 1 is untouch

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-141.5457, 430.599976, 5485.75049, -0.999583125, -7.39692974e-09, 0.0288711432, -7.10421233e-09, 1, 1.02413376e-08, -0.0288711432, 1.0031961e-08, -0.999583125)
    
    


end
end
end
end



function BlockDoor()
while getgenv().BlockDoor == true do
    wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2.96427131, 63.3200569, 71.4399185, -0.99997288, 1.04027169e-08, -0.00736289192, 1.10419185e-08, 1, -8.6773241e-08, 0.00736289192, -8.68521894e-08, -0.99997288)
    wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-6.63092041, 63.3200569, 71.4437103, -0.999958515, -7.34026173e-09, -0.00910972245, -7.85816479e-09, 1, 5.68159209e-08, 0.00910972245, 5.6885149e-08, -0.999958515)
    wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-9.29678059, 63.3200569, 71.4632645, -0.99997288, -1.19687504e-08, -0.00736288913, -1.2630534e-08, 1, 8.9834451e-08, 0.00736288913, 8.99250097e-08, -0.99997288)
    wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-10.6299963, 63.3200569, 71.4809036, -0.99977237, 1.72317494e-10, -0.0213350523, 2.90049401e-10, 1, -5.51514434e-09, 0.0213350523, -5.52007728e-09, -0.99977237)
    wait()
end
end

function V3()
while getgenv().NoMoreTpV3 == true do
local chara = game.Players.LocalPlayer.Character
            local origin = chara.HumanoidRootPart.CFrame
            wait()
            
            for _,v in pairs(game:GetService("Workspace").OldBuildingTp2:GetDescendants()) do
if v:IsA("TouchTransmitter") then
firetouchinterest(game.Players.LocalPlayer.Character.Torso, v.Parent, 0) --0 is touch
wait()
firetouchinterest(game.Players.LocalPlayer.Character.Torso, v.Parent, 1) -- 1 is untouch

chara.HumanoidRootPart.CFrame = origin


end
end
end
end
